		<script type="text/javascript">
			var mUrl, backUrl, backArr, backHost;
			$('#btn-back').click(function(){
				mUrl = "http://m.66rpg.com";
				// 获取上一页域名
				backUrl = document.referrer;
				backArr = backUrl.split("/");
				backHost = 'http://' + backArr[2];
				// 返回上一级域名是M站的话，返回上一页，不是调回首页
				if(backHost.indexOf(mUrl) >= 0){
					// 针对支付宝充值成功回调单独判断
					if(backUrl.indexOf('alipay') >= 0) {
						window.location.href = '/list/index';
						return false;
					}
					window.history.back();		
				}else{
					window.location.href = '/list/index';		
				}
			});
	
		</script>
